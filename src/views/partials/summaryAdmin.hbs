<h5 class='text-center'>ULTIMOS MOVIMIENTOS</h5>
<div class='accordion' id='accordionExample'>
  <div class='accordion-item'>
    <h4 class='accordion-header' id='headingSub'>
      <span
        class='accordion-button text-success'
        type='button'
        data-bs-toggle='collapse'
        data-bs-target='#collapseSub'
        aria-expanded='true'
        aria-controls='collapseSub'
      >
        Ultimas suscripciones registradas
      </span>
    </h4>
    <div
      id='collapseSub'
      class='accordion-collapse collapse show'
      aria-labelledby='headingSub'
      data-bs-parent='#accordionExample'
    >
      <div class='accordion-body'>
        <br />
        <table class='table table-striped table-hover' id='table'>
          <thead>
            <tr>
              <th scope='col'>#</th>
              <th scope='col'>Artista</th>
              <th scope='col'>Email</th>
              <th scope='col'>Fecha de compra</th>
              <th scope='col'>Plan</th>
              <th scope='col'>Precio</th>
            </tr>
          </thead>

          <tbody>
            {{#each response.latestSub}}
              <tr>
                <td>{{numbers}}</td>
                <td>{{seudonimo_art}}</td>
                <td>{{email_art}}</td>
                <td>{{dateFormat finico_sus}}</td>
                <td>{{nombre_pl}}</td>
                <td>{{coinFormat total_sus}}</td>
              </tr>
            {{/each}}
          </tbody>
          <tfoot>
            <tr>
              <td></td>
              <td></td>
              <td></td>
              <td></td>
              <td class='text-warning'>Total</td>
              <td class='text-warning'>{{coinFormat response.Total}}</td>
            </tr>
          </tfoot>
        </table>
      </div>
    </div>
  </div>

</div>
